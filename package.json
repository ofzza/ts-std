    "standard"
  ],
  "author": "ofzza",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ofzza/ts-std/issues"
  },
  "homepage": "https://github.com/ofzza/ts-std#readme",
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@types/jasmine": "~4.0.0",
    "@types/node": "^18.11.8",
    "jasmine": "~4.3.0",
    "jasmine-core": "~4.3.0",
    "jasmine-spec-reporter": "^7.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.7.1",
    "typescript": "~4.7.2"
  }
